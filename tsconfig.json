{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "resources/js/**/*",
    "resources/js/**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./resources/js/*"]
    },
    "types": [
      "vite/client"
    ]
  }
}